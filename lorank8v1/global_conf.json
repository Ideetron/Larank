        "stat_format": "semtech",                 /* semtech or idee_verbose or idee_concise. */
        "stat_damping": 50,                       /* 1 for least damping up to 99 for most damping. */
        "stat_file": "stats.txt",                 /* Put or the whole path, or only a file name */
